{
  "bindings": [
    {
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "route": "project/{status}",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "name": "res",
      "direction": "out"
  },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "projects",
      "ConnectionStringSetting": "legendstats_DOCUMENTDB",
      "databaseName": "icenter",
      "collectionName": "projects",
      "sqlQuery": "SELECT * from c where c.status = {status}"
    }
  ]
}
