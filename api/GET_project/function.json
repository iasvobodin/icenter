{
  "bindings": [
    {
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "route": "project/{partitionKeyValue}/{id}",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "name": "res",
      "direction": "out"
  },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "data",
      "ConnectionStringSetting": "legendstats_DOCUMENTDB",
      "databaseName": "testdatabase",
      "collectionName": "projects",
      "Id": "{id}",
      "PartitionKey": "{partitionKeyValue}"
    }
  ]
}
