{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "projectstatus"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "data",
      "ConnectionStringSetting": "legendstats_DOCUMENTDB",
      "databaseName": "icenter",
      "collectionName": "sharepoint",
      "sqlQuery": "SELECT * from c where NOT CONTAINS(c.status, {excludestatus}) AND NOT CONTAINS(c.status, '11')"
    }
  ]
}
