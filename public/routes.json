{
    "routes": [
        {
            "route": "/login",
            "serve": "/.auth/login/aad"
        },
        {
            "route": "/admin/*",
            "serve": "/index.html",
            "allowedRoles": ["admin"]
        },
        {
          "route": "/api/*"
        }
    ],
    "platformErrorOverrides": [
        {
          "errorType": "NotFound",
          "serve": "/custom-404.html"
        },
        {
          "errorType": "Unauthenticated",
          "statusCode": "302",
          "serve": "/login"
        }
      ]
}